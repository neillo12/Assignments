#include <stdio.h>
void merge ( int a[], int m, int b[], int n, int r[]);

int main(void){
int a1[] = {1,2,3,4};
int a2[] = {5,7,8,9};
int r[8];

merge(a1,4,a2,4,r);
for(int i = 0; i <8; i++)
   printf("r[%d]=%d\n", i, r[i]);

return 0;
}
void merge ( int a[], int m, int b[], int n, int r[]){
int i =0, j=0, k=0;
while (i<m && j<n){
if(a[i]<b[j]){
r[k]=a[i]
k++;
i++;
}
else{
r[k]=b[j]
k++;
j++;
}}
while(i<m){
r[k]=a[i]
k++;
i++;
}
wjile(j<n){
r[k]=b[i];
  k++;;}}


